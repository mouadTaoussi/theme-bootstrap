{
  "name": "@oruga-ui/theme-bootstrap",
  "version": "0.3.3",
  "description": "Bootstrap theme for Oruga",
  "license": "MIT",
  "main": "dist/bootstrap.js",
  "module": "dist/bootstrap.js",
  "unpkg": "dist/bootstrap.min.js",
  "sideEffects": [
    "*.css",
    "*.scss"
  ],
  "files": [
    "dist",
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/oruga-ui/theme-bootstrap.git"
  },
  "bugs": {
    "url": "https://github.com/oruga-ui/theme-bootstrap/issues"
  },
  "scripts": {
    "dev": "vite --force",
    "build": "vue-tsc --noEmit && vite build",
    "build:lib": "rimraf dist && rollup -c",
    "serve": "vite preview",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "style": "stylelint **/*.{css,scss} --fix --ignore-path .gitignore",
    "update": "ncu -u"
  },
  "dependencies": {
    "bootstrap": "^5.3.0"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-free": "6.4.0",
    "@oruga-ui/oruga-next": "0.6.0",
    "@rollup/plugin-commonjs": "25.0.0",
    "@rollup/plugin-typescript": "11.1.1",
    "@vitejs/plugin-vue": "4.2.3",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vue/tsconfig": "^0.4.0",
    "autoprefixer": "10.4.14",
    "core-js": "3.30.2",
    "eslint": "^8.41.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.14.1",
    "npm-check-updates": "^16.10.12",
    "postcss": "8.4.23",
    "prettier": "^2.8.8",
    "rimraf": "5.0.1",
    "rollup": "^2.68.0",
    "rollup-plugin-copy": "3.4.0",
    "rollup-plugin-sass": "1.12.19",
    "rollup-plugin-terser": "7.0.2",
    "sass": "1.62.1",
    "stylelint": "^15.6.2",
    "stylelint-config-recommended": "^12.0.0",
    "stylelint-config-recommended-scss": "^11.0.0",
    "stylelint-prettier": "^3.0.0",
    "stylelint-scss": "^5.0.0",
    "tslib": "2.5.2",
    "typescript": "5.0.4",
    "vite": "4.3.8",
    "vue": "3.3.4",
    "vue-router": "4.2.1",
    "vue-tsc": "1.6.5"
  }
}
